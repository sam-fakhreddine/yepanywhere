{
  "name": "@yep-anywhere/server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx --conditions source src/index.ts",
    "dev:watch": "tsx watch --conditions source src/index.ts",
    "dev:mock": "AUTH_DISABLED=true tsx --conditions source src/index.ts",
    "build": "tsc",
    "test": "vitest run",
    "test:e2e": "vitest run test/e2e",
    "test:e2e:real": "REAL_SDK_TESTS=true vitest run test/e2e",
    "test:e2e:real:fg": "REAL_SDK_TESTS=true FOREGROUND=1 vitest run test/e2e"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.12.0",
    "@anthropic-ai/claude-agent-sdk": "^0.2.29",
    "@hono/node-server": "^1.13.7",
    "@hono/node-ws": "^1.2.0",
    "@openai/codex-sdk": "^0.77.0",
    "@yep-anywhere/shared": "workspace:*",
    "bcrypt": "^6.0.0",
    "diff": "^8.0.2",
    "hono": "^4.6.14",
    "http-proxy": "^1.18.1",
    "marked": "^17.0.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "proper-lockfile": "^4.1.2",
    "shiki": "^3.20.0",
    "tssrp6a": "^3.0.0",
    "tweetnacl": "^1.0.3",
    "web-push": "^3.6.7",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/diff": "^8.0.0",
    "@types/http-proxy": "^1.17.17",
    "@types/marked": "^6.0.0",
    "@types/node": "^22.10.2",
    "@types/proper-lockfile": "^4.1.4",
    "@types/web-push": "^3.6.4",
    "@types/ws": "^8.18.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "ws": "^8.19.0"
  }
}
